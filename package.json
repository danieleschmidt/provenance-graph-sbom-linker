    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
