    "format": "make format",
    "build": "make build",
    "build:docker": "make docker-build",
    "clean": "make clean",
    "docs": "make docs",
    "docs:serve": "make docs-serve",
    "security": "make security-scan",
    "deps:update": "make deps-update",
    "deps:audit": "make deps-audit",
    "benchmark": "make benchmark",
    "profile": "make profile",
    "release": "make release",
    "pre-commit": "pre-commit run --all-files",
    "setup": "make setup",
    "dashboard": "cd web && npm run dev",
    "api:docs": "swag init -g cmd/server/main.go -o api/docs",
    "proto:generate": "make proto-generate",
    "mock:generate": "make mock-generate",
    "db:migrate": "make db-migrate",
    "db:seed": "make db-seed",
    "db:reset": "make db-reset",
    "k8s:deploy": "make k8s-deploy",
    "k8s:delete": "make k8s-delete",
