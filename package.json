{
  "name": "provenance-graph-sbom-linker",
  "version": "1.0.0",
  "description": "End-to-end software supply chain provenance tracker with cryptographic attestation",
  "main": "web/index.js",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up -d && make dev",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "test": "make test",
    "test:unit": "make test-unit",
    "test:integration": "make test-integration",
    "test:e2e": "make test-e2e",
    "test:coverage": "make test-coverage",
    "lint": "make lint",
